@model IEnumerable<Instructor>

<div class="container">
    <div class="table-wrapper">
        <table class="table table-striped table-hover">
            <thead>
                <tr>
                    <th>
                        @Html.DisplayNameFor(model => model.Id)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Specialization)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.WorkExp)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.Qualification)
                    </th>
                    <th>
                        @Html.DisplayNameFor(model => model.EducationInfo)
                    </th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(model => item.Id)
                        </td>
                        <td>
                            @Html.DisplayFor(model => item.Specialization)
                        </td>
                        <td>
                            @Html.DisplayFor(model => item.WorkExp)
                        </td>
                        <td>
                            @Html.DisplayFor(model => item.Qualification)
                        </td>
                        <td>
                            @Html.DisplayFor(model => item.EducationInfo)
                        </td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</div>